{
  "name": "subschema",
  "version": "1.1.5",
  "description": "Backbone Forms like declaritive forms for react",
  "main": "dist/subschema-server.js",
  "browser": "dist/subschema.js",
  "web": "dist/subschema-noreact.js",
  "webpack": "src/index.jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/subschema/subschema"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test-server": "NODE_ENV=test mocha ./test/**/*-test-server.js",
    "test": "NODE_ENV=test karma start --single-run --browsers Firefox",
    "test-dist": "NODE_ENV=test karma start ./karma.conf.js --single-run --browsers Firefox",
    "karma": "NODE_ENV=test karma start --no-single-run --colors --auto-watch",
    "karma-dist": "NODE_ENV=test karma start ./karma.conf.js --no-single-run --colors --auto-watch --browsers Firefox",
    "compile": "NODE_ENV=production webpack -p  --display-modules --config ./webpack.config.js",
    "prepublish": "npm prune && npm run test && npm run compile && npm run test-dist && npm run test-server"
  },
  "keywords": [
    "react",
    "forms",
    "backbone-forms"
  ],
  "author": "Justin Spears",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-dev-expression": "^0.1.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "expect": "^1.14.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.3",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "^0.1.2",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mocha": "^2.4.5",
    "postcss": "^5.0.19",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.0",
    "source-map": "^0.5.3",
    "source-map-support": "^0.4.0",
    "strip-loader": "^0.1.1",
    "style-loader": "^0.13.0",
    "subschema-test-support": "^1.1.5",
    "supports-color": "^3.1.2",
    "text-loader": "0.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.11.0"
  },
  "peerDependencies": {
    "react": "*",
    "lodash": "*",
    "fbjs": "*",
    "react-dom": "*",
    "react-addons-css-transition-group": "*",
    "subschema-injection": "*",
    "subschema-prop-types": "*"
  },
  "dependencies": {
    "fbjs": "^0.8.0-alpha.3",
    "lodash": "^3.10.1",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "subschema-injection": "^1.0.10",
    "subschema-prop-types": "^1.0.0"
  }
}
