{
  "name": "subschema",
  "version": "0.9.1",
  "description": "Backbone Forms like declaritive forms for react",
  "main": "src/index.jsx",
  "browser": "dist/subschema.js",
  "scripts": {
    "test": "karma start --single-run --browsers Firefox",
    "start": "webpack-dev-server --content-base ./public/",
    "hot": "node webpack-server.js",
    "karma": "karma start --no-single-run --colors --auto-watch",
    "docs": "node bin/docs.js",
    "prepublish": "webpack --config webpack.config.js -p",
    "postinstall": "node bin/postinstall.js",
    "build-demo": "webpack   --optimize-minimize  --optimize-occurence-order  --optimize-dedupe  --config ./demo.webpack.config.js"
  },
  "keywords": [
    "react",
    "forms",
    "backbone-forms"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jspears/subschema"
  },
  "author": "Justin Spears",
  "license": "MIT",
  "devDependencies": {
    "amdefine": "^0.1.0",
    "babel-core": "^5.1.11",
    "babel-loader": "^5.0.0",
    "compression-webpack-plugin": "^0.2.0",
    "css-loader": "^0.9.1",
    "dmd": "^1.0.3",
    "expect": "^1.6.0",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^0.5.0",
    "formatter.js": "^0.1.5",
    "highlight.js": "^8.5.0",
    "jsdoc-to-markdown": "^1.0.3",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.0",
    "less": "^2.4.0",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.1",
    "modernizr": "^3.0.0-alpha.4",
    "raw-loader": "^0.5.1",
    "react": "^0.13.1",
    "react-bootstrap": "^0.20.0",
    "react-highlight": "^0.4.1",
    "react-hot-loader": "^1.2.5",
    "react-router": "^0.13.2",
    "style-loader": "^0.9.0",
    "text-loader": "0.0.1",
    "url-loader": "^0.5.5",
