{
  "name": "subschema",
  "version": "0.52.1",
  "description": "Backbone Forms like declaritive forms for react",
  "main": "dist/subschema-server.js",
  "browser": "dist/subschema.js",
  "web": "dist/subschema-noreact.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/subschema/subschema"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "karma start --single-run --browsers Firefox",
    "test-dist": "karma start ./karma.dist.noreact.conf.js --single-run --browsers Firefox",
    "start": "webpack-dev-server --config ./hot.webpack.config",
    "hot": "node webpack-server.js",
    "karma": "karma start --no-single-run --colors --auto-watch",
    "karma-dist": "karma start ./karma.dist.noreact.conf.js --no-single-run --colors --auto-watch",
    "compile": "NODE_ENV=production webpack --config ./webpack.config.js -p --verbose",
    "prepublish": "NODE_ENV=production npm run test && NODE_ENV=production webpack --config ./webpack.config.js -p && npm run test-dist && node ./scripts/test-node.js",
    "demo": "webpack   --optimize-minimize  --optimize-occurence-order  --optimize-dedupe  --config ./demo.webpack.config.js"
  },
  "keywords": [
    "react",
    "forms",
    "backbone-forms"
  ],
  "author": "Justin Spears",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "autoprefixer-core": "^6.0.1",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.33",
    "babel-loader": "^5.3.3",
    "babel-plugin-dev-expression": "^0.1.0",
    "babel-runtime": "^5.8.29",
    "codemirror": "^5.8.0",
    "component-playground": "^1.0.3",
    "compression-webpack-plugin": "^0.2.0",
    "css-loader": "^0.21.0",
    "dmd": "^1.0.3",
    "es6-promise": "^3.0.2",
    "expect": "^1.6.0",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "fbjs": "^0.4.0",
    "file-loader": "^0.8.5",
    "formatter.js": "^0.1.5",
    "highlight.js": "^8.5.0",
    "history": "^1.15.0",
    "js-base64": "^2.1.9",
    "jsdoc-to-markdown": "^1.0.3",
    "json-loader": "^0.5.3",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "^0.1.1",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "lodash": "^3.10.1",
    "mocha": "^2.2.1",
    "modernizr": "^3.0.0-alpha.4",
    "postcss-color-rebeccapurple": "^2.0.0",
    "postcss-loader": "^0.7.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "source-map": "^0.5.3",
    "source-map-support": "^0.4.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "supports-color": "^3.1.2",
    "text-loader": "0.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.11.0"
  },
  "peerDependencies": {
    "react": "*",
    "lodash": "*"
  },
  "dependencies": {
    "fbjs": "^0.4.0",
    "react-dom": "^0.14.3"
  }
}
